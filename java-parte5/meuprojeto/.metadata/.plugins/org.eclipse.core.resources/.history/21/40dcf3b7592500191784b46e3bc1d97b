
public class TesteConexao {

	public static void main(String[] args) {
		Conexao con = null;
		try {
			con = new Conexao();
			con.leDados();
		} catch (IllegalStateException ex) {
			System.out.println("Deu error na conexao.");
		} finally { // esse bloco Ã© executado dando erro ou nao
			con.fecha();
		}
		

	}

}
